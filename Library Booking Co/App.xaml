<Application x:Class="Library_Booking_Co.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Library_Booking_Co"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <Style x:Key="Barcode">
            <Setter Property="TextElement.FontFamily"
                       Value="/Fonts/#Libre Barcode 39">
            </Setter>
        </Style>
        <Style x:Key="Jost">
            <Setter Property="TextElement.FontFamily"
                       Value="/Fonts/#Jost">
            </Setter>
        </Style>
        <Style x:Key="Century">
            <Setter Property="TextElement.FontFamily"
                       Value="/Fonts/#Century">
            </Setter>
        </Style>
        <Style x:Key="BebasKai">
            <Setter Property="TextElement.FontFamily"
                       Value="/Fonts/#Bebas Kai">
            </Setter>
        </Style>
        <Style x:Key="BebasNeue">
            <Setter Property="TextElement.FontFamily"
                       Value="/Fonts/#Bebas Neue">
            </Setter>
        </Style>




    </Application.Resources>


</Application>
